    "dayjs": "^1.11.10",
    "markdown-to-jsx": "^7.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
